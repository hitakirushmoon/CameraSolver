class Node {
	Node[] neighbors = new Node[4];
	int[] costs = new int[4];
	boolean[] hasThisNeighbor = new boolean[4];
	int x, y;

	public Node(int x, int y) {
		super();
		this.x = x;
		this.y = y;
	}
}