package simulation;

public class Creature extends Place {
	double direction;
	double speed;
	double size;
	double sense;
	double energy;
	World world;
	Place target;

	public Creature(World world, double posX, double posY, double speed, double size, double sense) {
		super(posX, posY);
		this.world = world;
		this.direction = Math.atan2(posY, posX);
		this.speed = speed;
		this.size = size;
		this.sense = sense;
		this.energy = 1;
	}

	void updateTarget() {
	}

	void move() {
	}

	void eat() {
	}

	void reproduce() {
	}
}
