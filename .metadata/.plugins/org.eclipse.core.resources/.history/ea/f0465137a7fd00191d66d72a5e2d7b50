package yeet;

public class QuickGame {
	static boolean[] arr = new boolean[52];
	static int[] choices = { 1, 3, 4 };

	public static void main(String[] args) {
		for (int i = 0; i < arr.length; i++) {
			if (!arr[i]) {
				for (int choice : choices) {
					arr[i + choice] = true;
				}
			}
		}
	}

}
